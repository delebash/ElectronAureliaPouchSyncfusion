{
  "name": "ElectronAureliaPouchSyncfusion",
  "title": "Electron ES6, Aurelia, AureliaSyncfusion,Syncfusion, PouchDB--LevelDown,",
  "version": "1.0.0",
  "description": "Electron Aurelia client using pouchdb--leveldown as backend and aurelia,bootstrap,syncfusion as front end.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-mac": "electron-packager . 'ElectronAureliaPouchSyncfusion' --platform=darwin --arch=x64 --version=1.4.2 --overwrite --out ./build/mac",
    "build-win": "electron-packager . 'ElectronAureliaPouchSyncfusion' --platform=win32 --arch=x64 --version=1.4.2 --overwrite --out ./build/win"
  },
  "author": "Daniel Elebash",
  "homepage": "http://ageektech.com",
  "license": "MIT",
  "keywords": [
    "electron",
    "aurelia",
    "es6",
    "pouchdb",
    "leveldown"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/delebash/ElectronAureliaPouchSyncfusion.git"
  },
  "devDependencies": {
    "electron": "^1.4.2",
    "electron-packager": "^8.1.0"
  }
}
